{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext" , "ES6" , "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext" ,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/test.jsx", "utils/motion.Ts", "components/Video.jsx", "components/Video.tsx"],
  "exclude": ["node_modules"]
}
